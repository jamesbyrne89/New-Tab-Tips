{
  "name": "CSS Tips Tab",
  "manifest_version": 2,
  "description": "Codecademy CSS tips when you open a new tab in Chrome",
  "version": "0.0.0.1",
  "permissions": ["management"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "assets/images/icon.png"
  },
  "content_security_policy": "script-src 'self' https://cdn.rawgit.com/google/code-prettify/master/loader/run_prettify.js?lang=css; object-src 'self'"
}
